<div class="subscription_wall">
  <div>
    <h1>Log In</h1>

    <%= form_tag sessions_path do %>
      <%= label_tag :username %><br>
      <%= text_field_tag :username %><br>

      <%= label_tag :password %><br>
      <%= password_field_tag :password %><br>

      <%= submit_tag 'Log In!' %>
    <% end %>
  </div>

  <div>
    <h1>Subscribe</h1>
    <%= button_to "Subscribe", nil, disabled: true %>
  </div>
</div>
